<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>UI/UX</title>
  <link rel="stylesheet"
    href="https://fonts.googleapis.com/css2?family=Material+Symbols+Sharp:opsz,wght,FILL,GRAD@48,400,0,0" />
  <link rel="stylesheet"
    href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined:opsz,wght,FILL,GRAD@20..48,100..700,0..1,-50..200" />
  <link rel="stylesheet" href="/homepage.css">
</head>

<body>
  <div class="container">
    <aside>
      <div class="top">
        <div class="logo">
          <h2>post <span class="danger">Ex</span> </h2>
        </div>
        <div class="close" id="close_btn">
          <span class="material-symbols-sharp">
            close
          </span>
        </div>
      </div>
      <div class="sidebar">
        <a href="#" class="active">
          <span class="material-symbols-sharp">grid_view </span>
          <h3>Dashboard</h3>
        </a>
        <a href="http://localhost:8080/view">
          <span class="material-symbols-outlined">local_shipping</span>
          <h3>Shipping</h3>
        </a>
        <a href="#">
          <span class="material-symbols-outlined">flight</span>
          <h3>Tracking</h3>
        </a>
        <a href="#">
          <span class="material-symbols-outlined">monitoring</span>
          <h3>Monitoring</h3>
        </a>
        <a href="#">
          <span class="material-symbols-outlined">attach_money</span>
          <h3>Payments</h3>
        </a>
        <a href="#">
          <span class="material-symbols-sharp">report_gmailerrorred </span>
          <h3>Reports</h3>
        </a>
        <a href="#">
          <span class="material-symbols-sharp">settings </span>
          <h3>Settings</h3>
        </a>
        <a href="#">
          <span class="material-symbols-outlined">account_circle</span>
          <h3>Admin Account</h3>
        </a>
        <a href="#">
          <span class="material-symbols-sharp">logout </span>
          <h3>Logout</h3>
        </a>
      </div>
    </aside>
    <main>
      <h1>Dashboard</h1>
      <div class="date">
        <input type="date">
      </div>
      <div class="insights">
        <div class="sales">
          <span class="material-symbols-sharp">trending_up</span>
          <div class="middle">

            <div class="left">
              <h3>Total Sales</h3>
              <h1>Rs. 650</h1>
            </div>
            <div class="progress">
              <svg>
                <circle r="30" cy="40" cx="40"></circle>
              </svg>
              <div class="number">
                <p>80%</p>
              </div>
            </div>
          </div>
          <small>Last 24 Hours</small>
        </div>
        <div class="expenses">
          <span class="material-symbols-sharp">local_mall</span>
          <div class="middle">
            <div class="left">
              <h3>Total Sales</h3>
              <h1>Rs. 25,024</h1>
            </div>
            <div class="progress">
              <svg>
                <circle r="30" cy="40" cx="40"></circle>
              </svg>
              <div class="number">
                <p>80%</p>
              </div>
            </div>
          </div>
          <small>Last month</small>
        </div>
        <!-- end seling -->
        <!-- start seling -->
        <div class="income">
          <span class="material-symbols-sharp">stacked_line_chart</span>
          <div class="middle">
            <div class="left">
              <h3>Total Sales</h3>
              <h1>Rs. 25,024</h1>
            </div>
            <div class="progress">
              <svg>
                <circle r="30" cy="40" cx="40"></circle>
              </svg>
              <div class="number">
                <p>80%</p>
              </div>
            </div>

          </div>
          <small>Last year</small>
        </div>
        <!-- end seling -->

      </div>
      <!-- end insights -->
      <div class="recent_order">
        <h2>Recent Shipments</h2>
        <table>
          <thead>
            <tr>
              <th>Shipment Name</th>
              <th>Shipment Number</th>
              <th>Payments</th>
              <th>Status</th>
            </tr>
          </thead>
          <tbody>
            <tr>
              <td>Speed Post</td>
              <td>4563</td>
              <td>Completed</td>
              <td class="warning">OnRoute</td>
              <td class="primary">Details</td>
            </tr>
            <tr>
              <td>Inland Letter</td>
              <td>4569</td>
              <td>Completed</td>
              <td class="warning">Delivered</td>
              <td class="primary">Details</td>
            </tr>
            <tr>
              <td>Inland Letter</td>
              <td>4570</td>
              <td>Due</td>
              <td class="warning">Delivered</td>
              <td class="primary">Details</td>
            </tr>
            <tr>
              <td>Speed Post</td>
              <td>4591</td>
              <td>Completed</td>
              <td class="warning">OnRoute</td>
              <td class="primary">Details</td>
            </tr>
          </tbody>
        </table>
        <a href="#">Show All</a>
      </div>

    </main>
    <!------------------
         end main
        ------------------->

    <!----------------
        start right main 
      ---------------------->
    <div class="right">

      <div class="top">
        <button id="menu_bar">
          <span class="material-symbols-sharp">menu</span>
        </button>

        <div class="theme-toggler">
          <span class="material-symbols-sharp active">light_mode</span>
          <span class="material-symbols-sharp">dark_mode</span>
        </div>
        <div class="profile">
          <div class="info">
            <p><b>AYUSH</b></p>
            <p>Admin</p>
            <small class="text-muted"></small>
          </div>
          <span class="material-symbols-outlined">account_circle</span> 
        </div>
      </div>

      <div class="recent_updates">
        <h2>Recent Update</h2>
        <div class="updates">
          <div class="update">
            <span class="material-symbols-outlined">
              person
              </span>
            <div class="message">
              <p><b>PABLO</b> Received his Order</p>
            </div>
          </div>
          <div class="update">
            <span class="material-symbols-outlined">
              person
              </span>
            <div class="message">
              <p><b>SAM</b> Received his Order</p>
            </div>
          </div>
          <div class="update">
            <span class="material-symbols-outlined">
              person
              </span>
            <div class="message">
              <p><b>JOHN</b> Placed Order</p>
            </div>
          </div>
        </div>
      </div>


      <div class="sales-analytics">
        <h2>Sales Analytics</h2>

        <div class="item onlion">
          <div class="icon">
            <span class="material-symbols-sharp">shopping_cart</span>
          </div>
          <div class="right_text">
            <div class="info">
              <h3>Online Shipments</h3>
              <small class="text-muted">Last seen 2 Hours</small>
            </div>
            <h5 class="danger">-17%</h5>
            <h3>3849</h3>
          </div>
        </div>
        <div class="item onlion">
          <div class="icon">
            <span class="material-symbols-sharp">shopping_cart</span>
          </div>
          <div class="right_text">
            <div class="info">
              <h3>Recent Shipments</h3>
              <small class="text-muted">Last seen 2 Hours</small>
            </div>
            <h5 class="success">12%</h5>
            <h3>3694</h3>
          </div>
        </div>
        <div class="item onlion">
          <div class="icon">
            <span class="material-symbols-sharp">shopping_cart</span>
          </div>
          <div class="right_text">
            <div class="info">
              <h3>Recent Shipment</h3>
              <small class="text-muted">Last seen 2 Hours</small>
            </div>
            <h5 class="danger">-12%</h5>
            <h3>4235</h3>
          </div>
        </div>



      </div>

      <div class="item add_product">
        <div>
          <span class="material-symbols-sharp">add</span>
        </div>
      </div>
    </div>


  </div>



  <script src="/homepage.js"></script>
</body>

</html>