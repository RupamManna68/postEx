<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>UI/UX</title>
    <link rel="stylesheet"
        href="https://fonts.googleapis.com/css2?family=Material+Symbols+Sharp:opsz,wght,FILL,GRAD@48,400,0,0" />
    <link rel="stylesheet"
        href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined:opsz,wght,FILL,GRAD@20..48,100..700,0..1,-50..200" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0/css/all.min.css">
    <link rel="stylesheet" href="shipperdata.css">
</head>

<body>
    <div class="container">
        <aside>

            <div class="top">
                <div class="logo">
                    <h2>post <span class="danger">Ex</span> </h2>
                </div>
            </div>
            <!-- end top -->
            <div class="sidebar">

                <a href="#">
                    <span class="material-symbols-sharp">grid_view </span>
                    <h3>Dashboard</h3>
                </a>
                <a href="#" class="active">
                    <span class="material-symbols-outlined">local_shipping</span>
                    <h3>Shipping</h3>
                </a>
                <a href="#">
                    <span class="material-symbols-outlined">flight</span>
                    <h3>Tracking</h3>
                </a>
                <a href="#">
                    <span class="material-symbols-outlined">monitoring</span>
                    <h3>Monitoring</h3>
                </a>
                <a href="#">
                    <span class="material-symbols-outlined">attach_money</span>
                    <h3>Payments</h3>
                </a>
                <a href="#">
                    <span class="material-symbols-sharp">report_gmailerrorred </span>
                    <h3>Reports</h3>
                </a>
                <a href="#">
                    <span class="material-symbols-sharp">settings </span>
                    <h3>Settings</h3>
                </a>
                <a href="#">
                    <span class="material-symbols-outlined">account_circle</span>
                    <h3>Admin account</h3>
                </a>
                <a href="#">
                    <span class="material-symbols-sharp">logout </span>
                    <h3>Logout</h3>
                </a>
            </div>
        </aside>

        <main>
            <div class="upper">
                <div class="shipment-delivery-analytics-main">
                    <div class="shipment-delivery-analytics">
                        <h1>Shipping Delivery Management</h1>
                        <div class="shipping-small">
                            <h4>Shipping</h4>
                            <i class="fa-solid fa-angle-right"></i>
                            <h4>Shipping Delivery Management</h4>
                        </div>
                    </div>
                    <div class="admin-name">
                        <i class="fa-solid fa-bell fa-2x"></i>
                        <i class="fa-solid fa-grip-lines-vertical fa-2x"></i>
                        <i class="fa-solid fa-circle fa-2x"></i>
                        <div class="actual-name">
                            <h3>Admin</h3>
                            <h4 style="color: grey;">admin@gmail.com</h4>
                        </div>
                        <i class="fa-solid fa-angle-down"></i>
                    </div>
                </div>

                <div class="filters">
                    <div class="filters-left">
                        <div class="shipper-date">
                            <label for="shipper-date">Filter by Shipper Date:</label>
                            <input type="date" id="shipper-date">
                        </div>

                        <div class="dropdown">
                            <div class="select">
                                <span class="selected">Filter by destination</span>
                                <div class="caret" style="color: black;"></div>
                            </div>
                            <ul class="menu">
                                <li>Filter by destination</li>
                                <li>Delhi</li>
                                <li>Kolkata</li>
                                <li>Mumbai</li>
                                <li>Chennai</li>
                            </ul>
                        </div>

                    </div>
                    <div class="filters-buttons">
                        <button id="add-new-page">Add New Shipping <i class="fa-solid fa-plus"></i></button>
                    </div>
                </div>
            </div>
            <div class="lower">
                <div class="analytics">
                    <div class="analytics-nav">
                        <div class="analytics-nav-left">
                            <i class="fa-solid fa-square-poll-vertical fa-3x"></i>
                            <h3>Shipment Delivery Analytics</h3>
                        </div>
                        <div class="analytics-nav-right">
                            <div class="courier-id">
                                <button id="search"><i class="fa-solid fa-magnifying-glass fa-1x"
                                        style="margin-right: 0.3rem; margin-left: 0.3rem;"></i></button>
                                <input type="textarea" style="width: 200px; height: 25px;" id="courier-id"
                                    placeholder="Enter Courier ID">
                            </div>
                            <div class="analytics-nav-right-filters">
                                <button><i class="fa-solid fa-filter"></i> Filter</button>
                            </div>
                            <button id="move"><i class="fa-solid fa-caret-left"></i></button>
                            <button id="move"><i class="fa-solid fa-caret-right"></i></button>
                        </div>
                    </div>
                </div>
                <table>
                    <thead>
                        <tr>
                            <th>Consg_No</th>
                            <th>Package Item</th>
                            <th>Service Type</th>
                            <th>Shipper Date</th>
                            <th>Departure</th>
                            <th>Destination</th>
                            <th>Weight</th>
                            <th>Status</th>
                        </tr>
                    </thead>
                    <tbody>
                        <% for(user of users) { %>
                            <tr>
                                <td>
                                    <%= user.consg_no %>
                                </td>
                                <td>
                                    <%= user.package_item %>
                                </td>
                                <td>
                                    <%= user.service_type %>
                                </td>
                                <td>
                                    <%= user.estd_delivery_date %>
                                </td>
                                <td>
                                    <%= user.sender_district %>
                                </td>
                                <td>
                                    <%= user.receiver_district %>
                                </td>
                                <td>
                                    <%= user.item_weight %>
                                </td>
                                <td>Just Ordered</td>
                            </tr>
                            <% } %>
                    </tbody>
                </table>
            </div>
            <div class="footer">
                <div class="footer-left">
                    <h5>Showing 10 of 250 entries</h5>
                </div>
                <div class="footer-right">
                    <button><i class="fa-solid fa-caret-left"></i></button>
                    <button id="one">1</button>
                    <button>2</button>
                    <button>...</button>
                    <button><i class="fa-solid fa-caret-right"></i></button>
                </div>
            </div>
        </main>
    </div>
    <script src="shipperdata.js"></script>
</body>

</html>